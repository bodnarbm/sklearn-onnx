# This is template for .pypirc file
# Copy this file to your $HOME directory each time you want to upload a new version
# cmdline: python -m twine upload dist/* --repository skl2onnx
[distutils]
index-servers =
    pypi
    skl2onnx

[pypi]
username = __token__
password = <not sure what to put here>

[skl2onnx]
repository = https://upload.pypi.org/legacy/
username = __token__
password = <skl2onnx project token here>
